{
  "version": 2,
  "devCommand": "",
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/[...mock].js" },
    { "handle": "filesystem" }
  ]
}